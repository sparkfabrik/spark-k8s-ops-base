      "customType": "regex",
      "managerFilePatterns": [
        "/^Dockerfile$/"
      ],
      "matchStrings": [
        "ARG\\s+ALPINE_VERSION=(?<currentValue>\\S+)"
      ],
      "datasourceTemplate": "docker",
      "packageNameTemplate": "alpine"
